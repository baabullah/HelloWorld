{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name" : "HelloWorld",
	"description": "Hope you enjoy this extension!",
	"version" : "1.0.0",
	"manifest_version" : 3,
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
      {
        "resources": [
          "inject.js",
          "jquery-3.6.0.min.js",
		  "wa-receive.mp3",
		  "wa-send.mp3"
        ],
        "matches": ["<all_urls>"]
      }
    ],
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ]
	
}